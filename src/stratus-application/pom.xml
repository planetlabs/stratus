    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <configId>minimal</configId>
        <configDirectory>${basedir}/../../../data</configDirectory>
        <webappSourceDirectory>${basedir}/src/main/webapp</webappSourceDirectory>
        <exec.jar.name>${project.build.finalName}-exec.jar</exec.jar.name>
        <docker.baseImage>adoptopenjdk/openjdk11:alpine-slim</docker.baseImage>
        <docker.jar.name>stratus-application-exec.jar</docker.jar.name>
        <docker.entryPoint>["java", "-jar", "-Duser.timezone=GMT", "/${docker.jar.name}"]</docker.entryPoint>
        <docker.imageName>stratus/stratus</docker.imageName>
        <stratusMainClass>stratus.StratusApplication</stratusMainClass>
    </properties>

