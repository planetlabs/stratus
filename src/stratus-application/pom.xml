    <!--
      The following profiles are active by default:
      geopkg,gwc,jp2k,mbstyle,vectortiles,wps
      You can deactivate these profiles by running the build with -Dminimal

      To enable GDAL support, build using `-P gdal` or `-Dgdal`
      -->
    <profiles>
        <!-- default profiles -->
        <profile>
            <id>geopkg</id>
            <dependencies>
                <dependency>
                    <groupId>org.geoserver.community</groupId>
                    <artifactId>gs-geopkg</artifactId>
                    <!-- Use stratus-gwc only -->
                    <exclusions>
                        <exclusion>
                            <groupId>org.geoserver</groupId>
                            <artifactId>gs-gwc</artifactId>
                        </exclusion>
                        <exclusion>
                            <groupId>org.geowebcache</groupId>
                            <artifactId>gwc-core</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
            </dependencies>
            <activation>
                <property>
                    <name>!minimal</name>
                </property>
            </activation>
        </profile>
        <profile>
            <id>gwc</id>
            <dependencies>
                <dependency>
                    <groupId>stratus</groupId>
                    <artifactId>stratus-gwc</artifactId>
                </dependency>
            </dependencies>
            <activation>
                <property>
                    <name>!minimal</name>
                </property>
            </activation>
        </profile>
        <profile>
            <id>jp2k</id>
            <dependencies>
                <dependency>
                    <groupId>org.geoserver.extension</groupId>
                    <artifactId>gs-jp2k</artifactId>
                </dependency>
            </dependencies>
            <activation>
                <property>
                    <name>!minimal</name>
                </property>
            </activation>
        </profile>
        <profile>
            <id>mbstyle</id>
            <dependencies>
                <dependency>
                    <groupId>org.geoserver.community</groupId>
                    <artifactId>gs-mbstyle</artifactId>
                </dependency>
            </dependencies>
            <activation>
                <property>
                    <name>!minimal</name>
                </property>
            </activation>
        </profile>
        <profile>
            <id>vectortiles</id>
            <dependencies>
                <dependency>
                    <groupId>org.geoserver.extension</groupId>
                    <artifactId>gs-vectortiles</artifactId>
                </dependency>
            </dependencies>
            <activation>
                <property>
                    <name>!minimal</name>
                </property>
            </activation>
        </profile>
        <profile>
            <id>wps</id>
            <dependencies>
                <dependency>
                    <groupId>org.geoserver.extension</groupId>
                    <artifactId>gs-wps-core</artifactId>
                </dependency>
                <dependency>
                    <groupId>org.geoserver.extension</groupId>
                    <artifactId>gs-web-wps</artifactId>
                </dependency>
                <dependency>
                    <groupId>org.geotools</groupId>
                    <artifactId>gt-wps</artifactId>
                </dependency>
                <dependency>
                    <groupId>stratus</groupId>
                    <artifactId>stratus-wps</artifactId>
                </dependency>
            </dependencies>
            <activation>
                <property>
                    <name>!minimal</name>
                </property>
            </activation>
        </profile>

        <!-- Web UI. Be sure to set stratus.admin-enabled=true to enable the UI -->
        <profile>
            <id>webadmin</id>
            <dependencies>
                <dependency>
                    <groupId>org.geoserver.web</groupId>
                    <artifactId>gs-web-app</artifactId>
                    <exclusions>
                        <exclusion>
                            <groupId>org.slf4j</groupId>
                            <artifactId>slf4j-log4j12</artifactId>
                        </exclusion>
                        <!-- Use stratus-gwc only -->
                        <exclusion>
                            <groupId>org.geoserver</groupId>
                            <artifactId>gs-gwc</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
                <dependency>
                    <groupId>org.geoserver.web</groupId>
                    <artifactId>gs-web-wcs</artifactId>
                </dependency>
                <dependency>
                    <groupId>commons-cli</groupId>
                    <artifactId>commons-cli</artifactId>
                    <version>1.3.1</version>
                </dependency>
                <!-- 1.11.18 is used here because newer things kick off a chain of dependency issues with GeoServer -->
                <dependency>
                    <groupId>com.amazonaws</groupId>
                    <artifactId>aws-java-sdk-s3</artifactId>
                    <!--<version>1.11.18</version>-->
                </dependency>
            </dependencies>
        </profile>
        <profile>
            <id>gdal</id>
            <dependencies>
                <dependency>
                    <groupId>org.geoserver.extension</groupId>
                    <artifactId>gs-gdal</artifactId>
                    <exclusions>
                        <exclusion>
                            <groupId>it.geosolutions.imageio-ext</groupId>
                            <artifactId>imageio-ext-gdal-bindings</artifactId>
                        </exclusion>
                    </exclusions>
                </dependency>
                <dependency>
                    <groupId>org.gdal</groupId>
                    <artifactId>gdal</artifactId>
                    <version>${gdal.version}</version>
                </dependency>
            </dependencies>
            <properties>
                <!-- https://github.com/planetlabs/stratus/blob/master/build/docker/amazonlinux-gdal-py-java11/Dockerfile -->
                <docker.baseImage>amazonlinux-gdal-py-java11:latest</docker.baseImage>
            </properties>
            <activation>
                <property>
                    <name>gdal</name>
                </property>
            </activation>
        </profile>
